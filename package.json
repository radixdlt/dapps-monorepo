{
  "name": "dashboard-radix",
  "version": "0.0.1",
  "engines": {
    "node": "16.17.1"
  },
  "scripts": {
    "dev": "vite serve --host 0.0.0.0",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "husky install",
    "svelte:check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "cypress": "cypress open",
    "prettier": "prettier --cache --check src ",
    "prettier:fix": "prettier --cache --write src",
    "lint": "eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "format": "yarn prettier:fix && yarn lint:fix",
    "e2etest": "cypress run",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "test:storybook": "test-storybook --no-index-json --coverage && nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook",
    "test:storybook:watch": "test-storybook --no-index-json --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "coverage": "vitest run --coverage",
    "update:gateway": "ts-node --esm ./scripts/update-gateway.ts"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@babel/core": "^7.20.12",
    "@popperjs/core": "^2.11.6",
    "@stitches/core": "^1.2.8",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-svelte-csf": "^2.0.11",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/jest": "^0.0.10",
    "@storybook/svelte": "^6.5.16",
    "@storybook/test-runner": "^0.9.4",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.104",
    "@sveltejs/adapter-static": "^1.0.0-next.49",
    "@sveltejs/eslint-config": "sveltejs/eslint-config",
    "@sveltejs/kit": "^1.0.0-next.442",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/ramda": "^0.28.22",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitebook/client": "^0.100.5",
    "@vitebook/core": "^0.100.5",
    "@vitebook/theme-default": "^0.100.5",
    "@vitest/coverage-c8": "^0.23.2",
    "@zerodevx/svelte-toast": "^0.8.0",
    "babel-loader": "^8.2.5",
    "cypress": "^10.10.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte": "^2.14.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jsdom": "^20.0.0",
    "json-schema-faker": "^0.5.0-rcv.46",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "request": "^2.88.2",
    "storybook-addon-mock": "^3.2.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.7.1",
    "svelte-loader": "^3.1.3",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^4.10.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.21.1"
  },
  "type": "module",
  "dependencies": {
    "@aleworm/svelte-skeleton-loader": "^0.0.2",
    "@radixdlt/babylon-gateway-api-sdk": "^1.0.0-beta.1.3",
    "@radixdlt/radix-dapp-toolkit": "0.1.0-alpha.10",
    "@rgossiaux/svelte-headlessui": "^1.0.2",
    "@tanstack/svelte-table": "^8.5.11",
    "bignumber.js": "^9.1.0",
    "crypto-js": "^4.1.1",
    "filepond": "^4.30.4",
    "filepond-plugin-file-validate-type": "^1.2.8",
    "ramda": "^0.28.0",
    "svelte-filepond": "^0.2.0",
    "svelte-local-storage-store": "^0.3.1"
  }
}
