{
  "name": "dashboard-radix",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync && husky install",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "cypress": "cypress open",
    "prettier": "prettier --check src/**",
    "prettier:fix": "prettier --write src/**",
    "lint": "eslint src",
    "pretest": "yarn lint & yarn prettier",
    "e2etest": "cypress run",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "vitebook:dev": "VITEBOOK=true vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview"
  },
  "devDependencies": {
    "@stitches/core": "^1.2.8",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/eslint-config": "sveltejs/eslint-config",
    "@sveltejs/kit": "next",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@testing-library/svelte": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitebook/client": "^0.100.5",
    "@vitebook/core": "^0.100.5",
    "@vitebook/theme-default": "^0.100.5",
    "cypress": "^10.3.1",
    "eslint": "^8.21.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.21.1"
  },
  "type": "module",
  "dependencies": {
    "bignumber.js": "^9.1.0",
    "radix-js": "./mock-sdk"
  }
}
