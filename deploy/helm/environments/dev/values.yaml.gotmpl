{{ $DASHBOARD_IMAGE_TAG := .StateValues.ci.tag }} 
image: 
    tag: {{$DASHBOARD_IMAGE_TAG}}

ingress:
  enabled: true
  annotations:
    kubernetes.io/ingress.class: nginx
  hosts:
    - host: dashboard-dev.radixdlt.com
      paths:
        - path: /
          pathType: "Prefix"
  tls:
    - secretName: self-signed-tls
      hosts:
        - dashboard-dev.radixdlt.com

